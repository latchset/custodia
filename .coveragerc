[run]
branch = True
source =
    custodia
    tests

[paths]
source =
   src/custodia
   .tox/*/lib/python*/site-packages/custodia

[report]
ignore_errors = False
precision = 1
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:
    if PY3
    if not PY3
